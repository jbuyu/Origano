    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.17",
    "react-toastify": "^7.0.3",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
